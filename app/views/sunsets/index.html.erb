<h1>Choose The Best Day to Enjoy a <%= @city.name %> Sunset</h1>
<% @sunsets.each do |sunset|  %>
<%= link_to city_places_path(sunset.city, :date_session => sunset.date) do %>

<h3><%= sunset.date %></h3>
<% end %>
<p>Sunset Timing: <%= ApplicationHelper.converted_time(sunset.start) %> - <%= ApplicationHelper.converted_time(sunset.end) %></p>

<p>Golden Hour: <%= ApplicationHelper.converted_time(sunset.gold_start) %> - <%= ApplicationHelper.converted_time(sunset.gold_end) %></p>
<br>
<% end %>
